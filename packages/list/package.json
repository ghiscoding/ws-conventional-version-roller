{
  "name": "@lerna-lite/list",
  "description": "Lerna-Lite list commmand - List local packages",
  "version": "1.17.0",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "build:incremental": "tsc --incremental --sourceMap false --excludeDirectories dist",
    "pack-tarball": "npm pack"
  },
  "license": "MIT",
  "author": "Ghislain B.",
  "homepage": "https://github.com/lerna-lite/lerna-lite",
  "repository": {
    "type": "git",
    "url": "https://github.com/lerna-lite/lerna-lite.git",
    "directory": "packages/list"
  },
  "bugs": {
    "url": "https://github.com/lerna-lite/lerna-lite/issues"
  },
  "engines": {
    "node": ">=16.15.0",
    "npm": ">=8.5.0"
  },
  "dependencies": {
    "@lerna-lite/core": "workspace:*",
    "@lerna-lite/filter-packages": "workspace:*",
    "@lerna-lite/listable": "workspace:*"
  },
  "devDependencies": {
    "yargs-parser": "^21.1.1"
  }
}
