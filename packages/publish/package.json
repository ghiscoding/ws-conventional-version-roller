{
  "name": "@lerna-lite/publish",
  "description": "Publish packages in the current workspace",
  "version": "2.5.0",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "build:incremental": "tsc --incremental --sourceMap false --excludeDirectories dist",
    "pack-tarball": "npm pack"
  },
  "license": "MIT",
  "author": "Ghislain B.",
  "homepage": "https://github.com/lerna-lite/lerna-lite",
  "repository": {
    "type": "git",
    "url": "https://github.com/lerna-lite/lerna-lite.git",
    "directory": "packages/publish"
  },
  "bugs": {
    "url": "https://github.com/lerna-lite/lerna-lite/issues"
  },
  "engines": {
    "node": ">=16.15.0",
    "npm": ">=8.5.0"
  },
  "dependencies": {
    "@lerna-lite/cli": "workspace:*",
    "@lerna-lite/core": "workspace:*",
    "@lerna-lite/version": "workspace:*",
    "@npmcli/arborist": "^6.3.0",
    "byte-size": "^8.1.1",
    "chalk": "^5.3.0",
    "columnify": "^1.6.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.4",
    "has-unicode": "^2.0.1",
    "libnpmaccess": "^7.0.2",
    "libnpmpublish": "^8.0.0",
    "normalize-path": "^3.0.0",
    "npm-package-arg": "^11.0.0",
    "npm-packlist": "^8.0.0",
    "npm-registry-fetch": "^16.0.0",
    "npmlog": "^7.0.1",
    "p-map": "^6.0.0",
    "p-pipe": "^4.0.0",
    "pacote": "^17.0.4",
    "pify": "^6.1.0",
    "read-package-json": "^7.0.0",
    "semver": "^7.5.4",
    "ssri": "^10.0.5",
    "tar": "^6.1.15",
    "temp-dir": "^3.0.0"
  },
  "devDependencies": {
    "@types/conventional-changelog-core": "^4.2.4",
    "@types/conventional-recommended-bump": "^6.1.1",
    "@types/fs-extra": "^11.0.1",
    "@types/pify": "^5.0.1",
    "@types/semver": "^7.5.1",
    "load-json-file": "^7.0.1",
    "write-pkg": "^6.0.0",
    "yargs": "^17.7.2",
    "yargs-parser": "^21.1.1"
  }
}
